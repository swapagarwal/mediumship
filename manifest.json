{
    "name": "Mediumship",
    "version": "2.0",
    "description": "Try Medium with membership-features!",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "webRequest",
        "*://*.medium.com/*",
        "webRequestBlocking"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "manifest_version": 2
}
