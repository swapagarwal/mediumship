{
    "name": "Mediumship",
    "version": "2.0",
    "description": "Try Medium with membership-features!",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "16": "icon16.png",
            "19": "icon19.png",
            "32": "icon32.png"
        },
        "default_title": "Read the full story!"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "activeTab",
        "<all_urls>"
    ],

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "manifest_version": 2
}
